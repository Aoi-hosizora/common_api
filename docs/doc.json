{
  "swagger": "2.0",
  "host": "localhost:10014",
  "basePath": "/",
  "info": {
    "title": "common_api",
    "description": "My common api collection",
    "version": "1.0",
    "contact": {
      "name": "Aoi-hosizora",
      "url": "https://github.com/Aoi-hosizora",
      "email": "aoihosizora@hotmail.com"
    }
  },
  "tags": [
    {
      "name": "Github",
      "description": "github-controller"
    }
  ],
  "paths": {
    "/github/rate_limit": {
      "get": {
        "summary": "Get rate limit status for the authenticated user",
        "operationId": "-github-rate_limit-get",
        "description": "See https://docs.github.com/en/rest/reference/rate-limit",
        "tags": [
          "Github"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "github token, format: Token xxx"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/github/raw": {
      "get": {
        "summary": "Get raw page without authentication",
        "operationId": "-github-raw-get",
        "tags": [
          "Github"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": true,
            "type": "string",
            "description": "Github url without github.com prefix"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/github/users/{name}/issues/timeline": {
      "get": {
        "summary": "Get github user issues timeline (event)",
        "operationId": "-github-users-name-issues-timeline-get",
        "description": "Fixed field: id?, node_id?, event(enum), actor(User), commit_id?, commit_url?, created_at(time), repo(string), number(integer), involve(string)",
        "tags": [
          "Github"
        ],
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "github username"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer",
            "description": "query page",
            "format": "int32"
          },
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "github token, format: Token xxx"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Result": {
      "type": "object",
      "required": [
        "code",
        "message"
      ],
      "description": "global response",
      "properties": {
        "code": {
          "type": "integer",
          "required": true,
          "description": "status code",
          "format": "int32"
        },
        "message": {
          "type": "string",
          "required": true,
          "description": "status message"
        }
      }
    }
  }
}
