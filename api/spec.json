  "paths": {
    "/github/rate_limit": {
      "get": {
        "summary": "Get rate limit status for the authenticated user",
        "operationId": "-github-rate_limit-get",
        "description": "See https://api.github.com/en/rest/reference/rate-limit",
        "tags": [
          "Github"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "description": "github token, format: Token xxx",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "200 OK",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/github/token/{token}/api/{url}": {
      "get": {
        "summary": "Request api with given token",
        "operationId": "-github-token-:token-api-:url-get",
        "tags": [
          "Github"
        ],
        "parameters": [
          {
            "name": "token",
            "in": "path",
            "required": true,
            "description": "github token, format: Token xxx",
            "type": "string"
          },
          {
            "name": "url",
            "in": "path",
            "required": true,
            "description": "github api url without api.github.com prefix",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "200 OK",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/github/users/{name}/issues/timeline": {
      "get": {
        "summary": "Get github user issues timeline (event)",
        "operationId": "-github-users-:name-issues-timeline-get",
        "description": "Fixed field: id?, node_id?, event(enum), actor(User), commit_id?, commit_url?, created_at(time), repo(string), number(integer), involve(string)",
        "tags": [
          "Github"
        ],
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "description": "github username",
            "type": "string"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "description": "query page",
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "description": "github token, format: Token xxx",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "200 OK",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    },
