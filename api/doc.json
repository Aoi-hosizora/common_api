          "description": "issue html url"
        },
        "state": {
          "type": "string",
          "description": "issue state"
        },
        "comments_count": {
          "type": "integer",
          "format": "int32",
          "description": "issue comments count"
        },
        "labels": {
          "type": "array",
          "description": "issue labels",
          "items": {
            "type": "string"
          }
        },
        "create_time": {
