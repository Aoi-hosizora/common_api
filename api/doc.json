        },
        "mobile_url": {
          "type": "string",
          "description": "notice url in mobile"
        },
        "type": {
          "type": "string",
          "description": "notice type, is some specific strings"
        },
        "date": {
          "type": "string",
          "format": "date",
          "description": "notice date, format: 0000-00-00"
        }
      }
