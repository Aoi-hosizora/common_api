      "type": "object",
      "required": [
        "title",
        "number",
        "url",
        "state",
        "comments_count",
        "labels",
        "create_time"
      ],
      "description": "github issue item",
      "properties": {
        "title": {
          "type": "string",
          "description": "issue title"
        },
        "number": {
